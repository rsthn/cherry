{
    "name": "@rsthn/cherry",
    "version": "2.0.27",
    "description": "Cherry Gamedev Library (JS)",
    "main": "./main.js",
    "exports": {
        ".": {
            "require": "./main.js",
            "default": "./main.mjs"
        },
        "./gx": {
            "require": "./gx.js",
            "default": "./gx.mjs"
        }
    },
    "scripts": {
        "test": "echo Ok",
        "requirements": "yarn global add --ignore-optional parcel-bundler",
        "deploy": "yarn build && node ./scripts/deploy.js",
        "build": "parcel build ./main.js -o cherry.js --global cherry --no-source-maps --no-cache"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/cherryjs.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/cherryjs/issues"
    },
    "homepage": "https://github.com/rsthn/cherryjs#readme",
    "browserslist": [
        "since 2019-01"
    ],
    "dependencies": {
        "@rsthn/rin": "^2.0.28",
        "filereader": "^0.10.3",
        "image-size": "^0.9.7"
    },
    "browser": {
        "./shims": false
    },
    "files": [
        "wrappers/",
        "controls/",
        "gx/",
        "*.js",
        "*.mjs",
        "LICENSE",
        "README.md"
    ]
}